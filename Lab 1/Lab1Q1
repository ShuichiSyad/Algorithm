package lab.pkg1.q1;

public class Lab1Q1 {

    public static void main(String[] args) {
        
        int[] a = {1, 2, 3};
        int[] b = {5, 11, 2};
        int[] c = {7, 0, 0};
        
        System.out.println(sum3(a));
        System.out.println(sum3(b));
        System.out.println(sum3(c));  
    }
    
    public static int sum3(int[] A)
    {
        int sum=0;
        for (int i = 0; i< A.length ; i++)
            sum += A[i];
     return sum;
        
    }
}
